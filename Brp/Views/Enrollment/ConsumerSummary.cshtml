
@using Brp.Models
@model Brp.Models.ConsumerResponse
@{
    ViewBag.Title = "- ConsumerSummary";
}



<!--page title start-->
<div class="page-heading">
    <div class="container-fluid">
        <div class="row d-flex align-items-center">
            <div class="col-md-6">
                <div class="page-breadcrumb">
                    @*<h1> Consumer Summary</h1>*@
                    &nbsp;
                </div>
            </div>
            <div class="col-md-6 justify-content-md-end d-flex">
                <div class="breadcrumb_nav">
                    <ol class="breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a class="parent-item" href="javascript:void(0);">Home</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li class="active">
                            Consumer Summary
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>
<!--page title end-->
<div class="container-fluid">
    <!-- state start-->
    <div class="row">
        <div class=" col-sm-12">
            <div class="card card-shadow mb-4">
                <div class="card-header">
                    <div class="card-title">
                        Consumer Summary
                    </div>
                </div>
                <div class="card-body">
                    <div id="detailedTable1"></div>
                    <table id="detailedTable" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <td>Partner Id</td>
                                <td>Consumer Id</td>
                                <td>Email</td>
                                <td>Identity Check Complete</td>
                                <td>Redirect Complete</td>
                                <td>Registered</td>
                                <td>Last Update</td>
                            </tr>
                            <tr>
                                <th>Partner Id</th>
                                <th>Consumer Id</th>
                                <th>Email</th>
                                <th>Identity Check Complete</th>
                                <th>Redirect Complete</th>
                                <th>Registered</th>
                                <th>Last Update</th>
                            </tr>
                        </thead>
                        @foreach (var item in Model.ConsumerStates)
                        {

                            <tr>
                                <td>@item.PartnerId </td>
                                <td>
                                    @item.ConsumerId<br />
                                    @item.Email <br />
                                    @item.IdentityCheckComplete <br />
                                </td>
                                <td>@item.PartnerId</td>
                                <td>@item.RedirectComplete</td>
                                <td>@item.Registered </td>
                                <td>@item.LastUpdate</td>
                                <td>@item.IdentityCheckComplete </td>
                            </tr>
                        }
                        <!--<tfoot>
                            <tr>
                                <th>Name</th>
                                <th>Position</th>
                                <th>Office</th>
                                <th>Age</th>
                                <th>Start date</th>
                                <th>Salary</th>
                            </tr>
                        </tfoot>-->
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!-- state end-->
</div>
<!-- Modal starts -->
